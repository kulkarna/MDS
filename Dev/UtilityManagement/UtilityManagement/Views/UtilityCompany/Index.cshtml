@model List<DataAccessLayerEntityFramework.usp_UtilityCompany_Index_Select_Result> 

@{
    ViewBag.Title = "Utility Company Index";

    
}

<div class="form-details">
    <div class="form-information">
        <div class="form-title-general">Index</div>
        <div class="form-title-specific float-left">Utility Company</div>
        <div class="float-right">
            <input type="text" class="text-box2"><div class="seachbtn">
                <button class="button" type="submit" value="Search for records" id="search">search</button></div>
        </div>
    </div>
    <div class="details-data-section">

        <div class="detail_table">
            <div class="left_container">
            <table class="index">
                <tr>
                    <th>Row Count = @Model.Count()</th>
                    <th>
                        @Html.ActionLink("Utility Code", "TitleClick", new { name = "UtilityCode" })
                        @{
                            if (!string.IsNullOrWhiteSpace(@ViewBag.UtilityCodeImageUrl))
                            {
                        <img id="imgUtilityCode" src="@ViewBag.UtilityCodeImageUrl"  valign="middle" />
                            }
                        }
                    </th>
                    <th>
                        @Html.ActionLink("Full Name", "TitleClick", new { name = "FullName" })
                        @{
                            if (!string.IsNullOrWhiteSpace(@ViewBag.FullNameImageUrl))
                            {
                        <img id="imgFullName" src="@ViewBag.FullNameImageUrl"  valign="middle" />
                            }
                        }
                    </th>
                    <th>
                        @Html.ActionLink("ISO", "TitleClick", new { name = "ISO" })
                        @{
                            if (!string.IsNullOrWhiteSpace(@ViewBag.IsoImageUrl))
                            {
                        <img id="imgIso" src="@ViewBag.IsoImageUrl"  valign="middle" />
                            }
                        }
                    </th>
                    <th>
                        @Html.ActionLink("Market", "TitleClick", new { name = "Market" })
                        @{
                            if (!string.IsNullOrWhiteSpace(@ViewBag.MarketImageUrl))
                            {
                        <img id="imgMarket" src="@ViewBag.MarketImageUrl"  valign="middle" />
                            }
                        }
                    </th>
                    <th>
                        @Html.ActionLink("Primary DUNS Number", "TitleClick", new { name = "PrimaryDunsNumber" })
