                </tr>
                @{
                    foreach (var item in Model)
                    {
                    <tr>
                        <td>
                            @{string checkBoxId = string.Format("CheckBoxSelection_{0}", item.Id);}
                            <input type="checkbox" class="check-box" id="@checkBoxId" name="SelectionCheckBox" onClick="UncheckAllButThis('@checkBoxId');">
                        <td>
                            @Html.DisplayFor(modelItem => item.UtilityCode)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FullName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ISO)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Market)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.PrimaryDunsNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.LpEntityId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.UtilityStatus)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EnrollmentLeadDays)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.BillingType)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AccountLength)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AccountNumberPrefix)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.PorOption)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.MeterNumberRequired)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.MeterNumberLength)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EdiCapable)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.UtilityPhoneNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Inactive)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CreatedBy)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CreatedDate)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.LastModifiedBy)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.LastModifiedDate)
                        </td>
                    </tr>
                    }
                }
            </table>
                </div></div>
                <div class="right_chart">
               </div>
        </div></div>
    </div>
</div>
@if (Session["ErrorMessage"] != null)
{<h3>@Session["ErrorMessage"].ToString()</h3>}
<script>
    $(function () {
        $("#tabs").tabs();
    });