                            }
                        }
                    </th>
                    <th>
                        @Html.ActionLink("Meter Number Required", "TitleClick", new { name = "MeterNumberRequired" })
                        @{
                            if (!string.IsNullOrWhiteSpace(@ViewBag.MeterNumberRequiredImageUrl))
                            {
                        <img id="imgMeterNumberRequired" src="@ViewBag.MeterNumberRequiredImageUrl"  valign="middle" />
                            }
                        }
                    </th>
                    <th>
                        @Html.ActionLink("Meter Number Length", "TitleClick", new { name = "MeterNumberLength" })
