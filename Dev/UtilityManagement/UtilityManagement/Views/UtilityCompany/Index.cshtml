                            {
                        <img id="imgPrimaryDunsNumber" src="@ViewBag.PrimaryDunsNumberImageUrl"  valign="middle" />
                            }
                        }
                    </th>
                    <th>
                        @Html.ActionLink("LP Entity Id", "TitleClick", new { name = "LpEntityId" })
                        @{
                            if (!string.IsNullOrWhiteSpace(@ViewBag.LpEntityIdImageUrl))
                            {
                        <img id="imgLpEntityId" src="@ViewBag.LpEntityIdImageUrl"  valign="middle" />
                            }
                        }
                    </th>
                    <th>
                        @Html.ActionLink("Utility Status", "TitleClick", new { name = "UtilityStatus" })
                        @{
                            if (!string.IsNullOrWhiteSpace(@ViewBag.UtilityStatusImageUrl))
                            {
                        <img id="imgUtilityStatus" src="@ViewBag.UtilityStatusImageUrl"  valign="middle" />
                            }
                        }
                    </th>
                    <th>
                        @Html.ActionLink("Enrollment Lead Days", "TitleClick", new { name = "EnrollmentLeadDays" })
