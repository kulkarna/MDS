                            if (!string.IsNullOrWhiteSpace(@ViewBag.EnrollmentLeadDaysImageUrl))
                            {
                        <img id="imgEnrollmentLeadDays" src="@ViewBag.EnrollmentLeadDaysImageUrl"  valign="middle" />
                            }
                        }
                    </th>
                    <th>
                        @Html.ActionLink("Billing Type", "TitleClick", new { name = "BillingType" })
                        @{
                            if (!string.IsNullOrWhiteSpace(@ViewBag.BillingTypeImageUrl))
                            {
                        <img id="imgBillingType" src="@ViewBag.BillingTypeImageUrl"  valign="middle" />
                            }
                        }
                    </th>
                    <th>
                        @Html.ActionLink("Account Length", "TitleClick", new { name = "AccountLength" })
                        @{
                            if (!string.IsNullOrWhiteSpace(@ViewBag.AccountLengthImageUrl))
                            {
                        <img id="imgAccountLength" src="@ViewBag.AccountLengthImageUrl"  valign="middle" />
                            }
                        }
                    </th>
                    <th>
                        @Html.ActionLink("Account Number Prefix", "TitleClick", new { name = "AccountNumberPrefix" })
                        @{
