@{
    ViewBag.Title = "Large File Upload";
    ViewBag.PageName = "LargeFileUpload";
}
<script type="text/javascript">
    function CuteWebUI_AjaxUploader_OnPostback() {
        //submit the form after the file have been uploaded:
        document.forms[0].submit();
    }
    alert(ViewBag.uploaderhtml);
</script>
<h2>Large File Upload in ASP.NET</h2>
<p>Ajax Uploader allows you to upload large files to a server with the low server memory consumption.</p>
<p>Click the following button to upload (No size/type restrictions).</p>
<div class="form-details">
    <div class="form-information">
        <div class="form-title-general">Index</div>
        <div class="form-title-specific float-left">File Download</div>
        <div class="float-right">
            <input type="text" class="text-box2">
                <div class="seachbtn">
                    <button class="button" type="submit" value="Search for records" id="search">search</button>
                </div>
        </div>
    </div>
    <div class="details-data-section">
        <div class="detail_table">
            <div class="left_container">

                <table class="index">
                    <tr>
                        <td>
                            @{Html.BeginForm();}
	                            @Html.Raw(ViewBag.uploaderhtml)
                            @{Html.EndForm();}
                            <div>
                                Server Trace:<br />
	                            @if (ViewBag.UploadedMessage != null)
                             {
		                            @ViewBag.UploadedMessage  
                             }
                            </div>
                        </td>
                    </tr>
                </table>

            </div>
        </div>
        <div class="right_chart">         
        </div>
    </div>
</div>
@if (Session["ErrorMessage"] != null)
{<h3>@Session["ErrorMessage"].ToString()</h3>}
<script>
    $(function () {
        $("#tabs").tabs();
    });
