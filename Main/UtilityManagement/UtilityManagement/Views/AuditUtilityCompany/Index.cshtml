        <th>
            @Html.ActionLink("Last Mod Date", "LastModifiedDateTitleClick")
        </th>
        <th>
            @Html.ActionLink("Last Mod Date Prev", "LastModifiedDatePrevClick")
        </th>
        <th>
            @Html.ActionLink("Change Version", "ChangeVersionClick")
        </th>
        <th>
            @Html.ActionLink("Creation Version", "CreationVersionClick")
        </th>
        <th>
            @Html.ActionLink("Change Operation", "ChangeOperationClick")
        </th>
        <th>
            @Html.ActionLink("Change Columns", "ChangeColumnsClick")
        </th>
      @*  <th>Row Count = @Model.Count()</th>*@
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPrevious)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UtilityCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UtilityCodePrevious)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Inactive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InactivePrevious)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedByPrevious)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedDatePrevious)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastModifiedBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastModifiedByPrevious)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastModifiedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastModifiedDatePrevious)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SYS_CHANGE_VERSION)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SYS_CHANGE_CREATION_VERSION)
            </td>
            <td>
