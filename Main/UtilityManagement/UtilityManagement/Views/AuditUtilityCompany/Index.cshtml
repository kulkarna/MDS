@model IEnumerable<DataAccessLayerEntityFramework.zAuditUtilityCompany>

@{
    ViewBag.Title = "Index";
}
<div class="form-details p15">
<div class="form-information">
        <div class="form-title-specific float-left">Audit Utility Index</div>
        <div class="float-right">
            <input type="text" class="text-box2"><div class="seachbtn">
                <button class="button" type="submit" value="Search for records" id="search">search</button>
            </div>
        </div>
    </div>

 <div class="details-data-section">
 <div class="details-full-section">
<div class="left_audit">
<table class="index">
    <tr>
        <th>
            @Html.ActionLink("Id", "IdClick")
        </th>
        <th>
            @Html.ActionLink("Id Prev", "IdPrevClick")
        </th>
        <th>
            @Html.ActionLink("Util Code", "UtilityCodeClick")
        </th>
        <th>
            @Html.ActionLink("Util Code Prev", "UtilityCodePrevClick")
        </th>
        <th>
            @Html.ActionLink("Inactive", "InactiveTitleClick")
        </th>
        <th>
            @Html.ActionLink("Inactive Prev", "InactivePrevClick")
        </th>
        <th>
            @Html.ActionLink("Create By", "CreatedByTitleClick")
        </th>
        <th>
            @Html.ActionLink("Create By Prev", "CreatedByPrevClick")
        </th>
        <th>
            @Html.ActionLink("Create Date", "CreatedDateTitleClick")
        </th>
        <th>
            @Html.ActionLink("Create Date Prev", "CreatedDatePrevClick")
        </th>
        <th>
            @Html.ActionLink("Last Mod By", "LastModifiedByTitleClick")
